{
	"name": "dlr-baselines-sandbox",
	"context": "..",
	"dockerFile": "Dockerfile",
	"settings": { 
		"terminal.integrated.shell.linux": "/bin/bash",
		"python.pythonPath": "/root/venv/bin/python3",
		"python.linting.enabled": true,
		"python.linting.pylintPath": "/root/venv/bin/pylint"
	},
	"extensions": [
		"ms-python.python",
		"ms-azuretools.vscode-docker"
	],
	"mounts": ["source=${localEnv:HOME}${localEnv:USERPROFILE}/.ssh,target=/home/vscode/.ssh,type=bind,consistency=default"]
}